class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        int c = 1;
        vector<vector<int>> ans;
        for(int i = 1; i <= numRows; ++i)
        {
            vector<int> sub;
            c = 1;
            for(int j = 1; j <= i; ++j)
            {
                sub.push_back(c);
                c = (c*(i - j))/j;
            }
            ans.push_back(sub);
        }
        return ans;
    }
};
