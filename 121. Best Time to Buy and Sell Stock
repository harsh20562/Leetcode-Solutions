class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n = prices.size();
        if(n==0 || n==1)
        {
            return 0;
        }
        int maxi = prices[n-1];
        int ans = 0;
        for(int i = n-2; i >= 0; --i)
        {
            ans = std::max(ans, maxi - prices[i]);
            if(prices[i] > maxi)
            {
                maxi = prices[i];
            }
        }
        return ans;
    }
};
