class Solution {
    public int removeCoveredIntervals(int[][] intervals) {
        int n = intervals.length;
        int ans = 0;
        for(int i=0; i<n; ++i)
        {
            for(int j=0; j<n; ++j)
            {
                if(j!=i)
                {   
                    if(intervals[i][0]>=intervals[j][0] && intervals[i][1]<=intervals[j][1])
                    {
                        ans = ans + 1;
                        break;
                    }
                }
            }
        }
        return n-ans;
    }
}
