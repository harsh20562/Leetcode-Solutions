class Solution {
public:
    int removePalindromeSub(string s) {
        int x = check(s);
        if(x==1)
        {
            return 1;
        }
        else
        {
            return 2;
        }
    }
    int check(string s)
    {
        int n = s.length();
        for(int i=0; i<n/2; ++i)
        {
            if(s[i]!=s[n-i-1])
            {
                return 0;
            }
        }
        return 1;
    }
};
