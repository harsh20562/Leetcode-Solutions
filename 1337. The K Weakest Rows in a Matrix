class Solution:
    def kWeakestRows(self, mat: List[List[int]], k: int) -> List[int]:
        dic = {}
        for i in range(len(mat)):
            summ = 0
            for j in range(len(mat[i])):
                summ = summ + mat[i][j]
            dic[i] = summ
        dic = sorted(dic.items(),key=lambda kv:(kv[1] , kv[0]))
        lst = []
        for i in dic:
            lst.append(i[0])
            k = k - 1
            if k==0:
                break
        return lst
