class Solution:
    def singleNumber(self, nums: List[int]) -> int:
        if len(nums)==1:
            return nums[0]
        nums.sort()
        idx = -1
        for i in range(1,len(nums)-1):
            if nums[i]!=nums[i+1] and nums[i]!=nums[i-1]:
                idx = i
        if idx==-1:
            if nums[0]!=nums[1]:
                idx = 0
            else:
                idx = len(nums)-1
        return nums[idx]
