/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
        unordered_set<ListNode*> v;
        while(head!=NULL)
        {
            if(v.find(head)==v.end())
            {
                v.insert(head);
                head = head->next;
            }
            else
            {
                return head;
            }
        }
        return NULL;
    }
};
