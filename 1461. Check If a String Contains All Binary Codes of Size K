class Solution {
public:
    bool hasAllCodes(string s, int k) {
        if(k>s.length())
        {
            return false;
        }
        unordered_map<string,int> mp;
        for(int i=0; i<=s.length()-k; ++i)
        {
            string n = "";
            for(int j=i; j<i+k; ++j)
            {
                n.push_back(s[j]);
            }
            // cout<<n<<" ";
            mp[n]++;
        }
        if(mp.size()==pow(2,k))
        {
            return true;
        }
        else
        {
            return false;
        }
    }
};
