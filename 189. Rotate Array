class Solution {
    public void rotate(int[] a, int k) {
        int n = a.length;
        if(k==n)
        {
            return;
        }
        else if(k>n)
        {
            k = k%n;
        }
        int[] result = new int[n];
        int j = n-k;
        for(int i=0; i<k; ++i)
        {
            result[i] = a[i+n-k];
        }
        for(int i=k; i<n; ++i)
        {
            result[i] = a[i-k];
        }
        for(int i=0; i<n; ++i)
        {
            a[i] = result[i];
        }
        return;
    }
}
