class Solution {
public:
    int countPrimes(int n) {
        if(n==0 || n==1 || n==2)
        {
            return 0;
        }
        int ans[n];
        for(int i=0; i<n; ++i)
        {
            ans[i] = 0;
        }
        int count = 0;
        for(int i=2; i<n; ++i)
        {
            if(ans[i]==0)
            {
                count = count + 1;
                for(int j=2; j*i<n; ++j)
                {
                    ans[j*i] = 1;
                }
            }
        }
        return count;
    }
};
