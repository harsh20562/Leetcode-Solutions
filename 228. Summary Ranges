class Solution {
public:
    vector<string> summaryRanges(vector<int>& v) {
        vector<string> ans;
        int n = v.size();
        if(n==0)
        {
            return ans;
        }
        else if(n==1)
        {
            ans.push_back(to_string(v[0]));
            return ans;
        }
        string s = "";
        int start=0,end=0;
        for(int i=0; i<=n-2; ++i)
        {
            if(v[i]+1==v[i+1])
            {
                end = i+1;
                if(end==n-1)
                {
                    if(start==end)
                    {
                        s = "";
                        s = s + to_string(v[start]);
                    }
                    else
                    {
                        s = "";
                        s = s + to_string(v[start]);
                        s = s + "->";
                        s = s + to_string(v[end]);
                    }
                    ans.push_back(s);
                    break;
                }
            }
            else
            {
                if(start==end)
                {
                    s = s + to_string(v[start]);
                }
                else
                {
                    s = s + to_string(v[start]);
                    s = s + "->";
                    s = s + to_string(v[end]);
                }
                ans.push_back(s);
                start = i+1;
                end = start;
                s = "";
            }
        }
        if(v[n-2]+1!=v[n-1])
        {
            ans.push_back(to_string(v[n-1]));
        }
        return ans;
    }
};
