class Solution {
public:
    vector<int> searchRange(vector<int>& nums, int target) {
        vector<int> ans;
        ans.push_back(first(nums,target));
        ans.push_back(second(nums,target));
        return ans;
    }
    int first(vector<int>& a, int target)
    {
        int l = 0;
        int r = a.size()-1;
        int ans = -1;
        while(l<=r)
        {
            int mid = (l+r)/2;
            if(a[mid]==target)
            {
                ans = mid;
                r = mid - 1;
            }
            else if(a[mid] < target)
            {
                l = mid + 1;
            }
            else if(a[mid] > target)
            {
                r = mid - 1;
            }
        }
        return ans;
    }
    int second(vector<int>& a, int target)
    {
        int l = 0;
        int r = a.size()-1;
        int ans = -1;
        while(l<=r)
        {
            int mid = (l+r)/2;
            if(a[mid]==target)
            {
                ans = mid;
                l = mid + 1;
            }
            else if(a[mid] < target)
            {
                l = mid + 1;
            }
            else if(a[mid] > target)
            {
                r = mid - 1;
            }
        }
        return ans;
    }
};
