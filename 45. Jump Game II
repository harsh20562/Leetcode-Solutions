class Solution {
public:
    int jump(vector<int>& nums) {
        int n = nums.size();
        int dp[n];
        dp[n-1] = 0;
        for(int i = n-2; i>=0; --i)
        {
            int mini = INT_MAX;
            for(int j = i+1; j <= min(n-1, i+nums[i]); ++j)
            {
                mini = min(mini, dp[j]);
            }
            if(mini!=INT_MAX)
            {
                mini = mini + 1;
            }
            dp[i] = mini;
        }
        return dp[0];
    }
};
