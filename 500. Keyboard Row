class Solution:
    def findWords(self, words: List[str]) -> List[str]:
        s1 = 'qwertyuiop'
        s2 = 'asdfghjkl'
        s3 = 'zxcvbnm'
        ans = []
        for i in range(len(words)):
            temp = words[i].lower()
            flag1 = 1
            flag2 = 1
            flag3 = 1
            for j in range(len(temp)):
                if temp[j] not in s1:
                    flag1 = 0
            for j in range(len(temp)):
                if temp[j] not in s2:
                    flag2 = 0
            for j in range(len(temp)):
                if temp[j] not in s3:
                    flag3 = 0
            if flag1==1 or flag2==1 or flag3==1:
                ans.append(words[i])
        return ans
        
