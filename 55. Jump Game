class Solution {
public:
    bool canJump(vector<int>& nums) {
        int n = nums.size();
        bool dp[n];
        int last = n-1;
        dp[n-1] = true;
        for(int i = n-2; i>=0; --i)
        {
            if(nums[i] >= last - i)
            {
                dp[i] = true;
                last = i;
            }
            else
            {
                dp[i] = false;
            }
        }
        return dp[0];
    }
};
