class Solution {
public:
    int uniquePaths(int m, int n) {
        long long int dp[m][n];
        dp[0][0] = 1;
        for(int i=0; i<m; ++i)
        {
            for(int j=0; j<n; ++j)
            {
                if(i==0 && j==0)
                {
                    continue;
                }
                else
                {
                    int sum = 0;
                    if(i-1>=0)
                    {
                        sum = sum + dp[i-1][j];
                    }
                    if(j-1>=0)
                    {
                        sum = sum + dp[i][j-1];
                    }
                    dp[i][j] = sum;
                }
                // cout<<dp[i][j]<<" ";
            }
            // cout<<"\n";
        }
        return dp[m-1][n-1];
    }
};
