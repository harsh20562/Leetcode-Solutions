class Solution {
public:
    int uniquePathsWithObstacles(vector<vector<int>>& a) {
        int n = a.size();
        int m = a[0].size();
        int dp[n][m];
        for(int i = 0; i < n; ++i)
        {
            for(int j = 0; j < m; ++j)
            {
                dp[i][j] = 0;
            }
        }
        if(a[0][0]==0)
        {
            dp[0][0] = 1;
        }
        else
        {
            dp[0][0] = 0;
        }
        for(int i = 0; i < n; ++i)
        {
            for(int j = 0; j < m; ++j)
            {
                if(i==0 && j==0)
                {
                    continue;
                }
                if(a[i][j]==1)
                {
                    dp[i][j] = 0;
                }
                else
                {
                    int s1 = 0, s2 = 0;
                    if(i-1>=0)
                    {
                        s1 = dp[i-1][j];
                    }
                    if(j-1>=0)
                    {
                        s2 = dp[i][j-1];
                    }
                    dp[i][j] = s1 + s2;
                }
            }
        }
        return dp[n-1][m-1];
    }
};
