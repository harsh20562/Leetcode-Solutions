class Solution {
public:
    bool backspaceCompare(string s, string t) {
        string ans1 = "";
        string ans2 = "";
        stack<char> s1;
        for(int i = 0; i < s.length(); ++i)
        {
            if(s[i]=='#')
            {
                if(s1.empty()==true)
                {
                    continue;
                }
                else
                {
                    s1.pop();
                }
            }
            else
            {
                s1.push(s[i]);
            }
        }
        while(!s1.empty())
        {
            ans1 = ans1 + s1.top();
            s1.pop();
        }
        for(int i = 0; i < t.length(); ++i)
        {
            if(t[i]=='#')
            {
                if(s1.empty()==true)
                {
                    continue;
                }
                else
                {
                    s1.pop();
                }
            }
            else
            {
                s1.push(t[i]);
            }
        }
        while(!s1.empty())
        {
            ans2 = ans2 + s1.top();
            s1.pop();
        }
        return ans1==ans2;
    }
};
