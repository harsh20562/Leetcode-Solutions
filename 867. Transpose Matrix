class Solution {
public:
    vector<vector<int>> transpose(vector<vector<int>>& matrix) {
        int rows = matrix.size();
        int cols = matrix[0].size();
        vector<vector<int>> ans;
        for(int i=0; i<cols; ++i)
        {
            vector<int> temp;
            for(int j = 0; j<rows; ++j)
            {
                temp.push_back(matrix[j][i]);
            }
            ans.push_back(temp);
        }
        return ans;
    }
};
