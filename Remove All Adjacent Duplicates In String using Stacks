class Solution {
    public String removeDuplicates(String s) {
        Stack<Character> stack = new Stack<Character>();
        for(int i=0; i<s.length(); ++i)
        {
            if(stack.empty()==true)
            {
                stack.push(s.charAt(i));
            }
            else if(stack.peek()==s.charAt(i))
            {
                stack.pop();
            }
            else
            {
                stack.push(s.charAt(i));
            }
        }
        String ans = "";
        while(stack.empty()!=true)
        {
            ans = stack.pop() + ans;
        }
        return ans;
    }
}
